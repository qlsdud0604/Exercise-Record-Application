<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Steadily.exerciseapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="Steadily"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".settingPage.GraphStyle_Activity"
            android:noHistory="true" />
        <activity
            android:name=".settingPage.TotalDays_Activity"
            android:noHistory="true" />
        <activity
            android:name=".settingPage.AverageTime_Activity"
            android:noHistory="true" />
        <activity
            android:name=".settingPage.LimitLine_Activity"
            android:noHistory="true" />

        <activity
            android:name=".page.Register_Activity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".page.Graph_Activity"
            android:noHistory="true" />
        <activity
            android:name=".page.BMI_Activity"
            android:noHistory="true" />
        <activity
            android:name=".page.Setting_Activity"
            android:noHistory="true" />
    </application>

</manifest>